<doctrine-mapping>
    <entity name = "App\Entity\User" table = "user" repository-class = "App\Repository\UserRepository">
        <id name = "userId" type = "integer" column = "user_id">
            <generator strategy = "IDENTITY" /> 
        </id>

        <field name = "firstName" type = "string" length = "20" column = "first_name" />
        <field name = "lastName" type = "string" length = "20" column = "last_name" />
        <field name = "contact" type = "string" length = "10" column = "contact" />
        <field name = "city" type = "string" length = "20" column = "city" />
    </entity>
</doctrine-mapping>